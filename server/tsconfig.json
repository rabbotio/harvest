{
  "compilerOptions": {
    "experimentalDecorators": true,
    "noEmitOnError": false,
    "moduleResolution": "node",
    "target": "es5",
    "allowSyntheticDefaultImports": true,
    "declaration": false,
    "skipDefaultLibCheck": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "jsx": "react",
    "lib": [
      "es5",
      "es6",
      "es7",
      "dom",
      "es2015.core",
      "es2015.collection",
      "es2015.generator",
      "es2015.iterable",
      "es2015.promise",
      "es2015.proxy",
      "es2015.reflect",
      "es2015.symbol",
      "es2015.symbol.wellknown",
      "es2017.object",
      "esnext.asynciterable"
    ]
  },
  "exclude": [
    "lib/fetcher/node_modules",
    "lib/fetcher/node_modules/**",
    "node_modules",
    "node_modules/**",
    "dist",
    ".next",
    "bin",
    "components/common"
  ],
  "compileOnSave": true
}